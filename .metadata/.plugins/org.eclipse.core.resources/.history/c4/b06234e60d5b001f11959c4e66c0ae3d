import java.util.Scanner;

public class Hello {

	// Ctrl + F11 - Executar
	// Ctrl + Barra de espaço - Assistente de Conteúdo (dicas de comandos)
	// Shift + Ctrl + F - Formata o código
	// syso e main - Atalhos
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.println("Digite um número:");
		int x = in.nextInt();
		System.out.println("Digite outro número:");
		int y = in.nextInt();
		System.out.println("Soma desses números: " + (x + y));
		System.out.println("dskjhdoahsdojsdao" + x + y);
		in.close();
	}

}
